{
  "name": "module-sandbox-template",
  "version": "1.0.0",
  "description": "Module development sandbox for React Admin Foundation",
  "type": "module",
  "scripts": {
    "dev:api": "tsx sandbox/server.ts",
    "dev:web": "vite --port 5173",
    "dev": "concurrently -k -n api,web -c blue,green \"npm:dev:api\" \"npm:dev:web\"",
    "build": "vite build",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "@types/pg": "^8.15.5",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "concurrently": "^9.1.0",
    "cors": "^5.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "drizzle-kit": "^0.31.4",
    "@types/cors": "^2.8.18"
  }
}